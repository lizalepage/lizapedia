<h1><%= @wiki.title %></h1>

<div class="row">
  <div class='col-md-8'>
    <h4> Orginally constructed by: <%= @wiki.user.email %> </h4>
    <%= @wiki.body %>
  </div>
  <div class= "col-md-4">
    <%= link_to "edit", edit_wiki_path(@wiki), class: 'btn btn-success' %>
    <%= link_to "delete", @wiki, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this post?'} %>
    
  </div>
</div>
